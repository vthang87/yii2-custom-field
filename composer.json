{
  "name": "vthang87/yii2-customfield",
  "description": "Yii2 Custom Field",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "custom",
    "field"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Thang Dang",
      "email": "vthang87@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "kartik-v/yii2-grid": "@dev",
    "yiisoft/yii2-jui": "~2.0.0",
    "kartik-v/yii2-widgets": "*"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "vthang87\\customfield\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
